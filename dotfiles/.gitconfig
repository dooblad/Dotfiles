[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
[user]
	name = Logan Weber
	email = weberlo@cs.washington.edu
[push]
	default = simple
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
