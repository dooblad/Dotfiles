export PATH="/usr/local/opt/python/libexec/bin:$PATH"
export PATH="$PATH:$HOME/.cargo/bin"
export PATH="$PATH:$HOME/bin"
export PATH="$PATH:$HOME/.local/bin"
export PATH="$PATH:$HOME/Library/Python/3.7/bin"
export PATH="$PATH:$(brew --prefix llvm)/bin"
export PATH="$PATH:$HOME/.emacs.d/bin"

export LIBRARY_PATH="$LIBRARY_PATH:/usr/local/lib"

export TVM_HOME="$HOME/Code/repo/sampl/tvm"
export RELAY_AOT_HOME="$HOME/Code/repo/sampl/relay-aot"

# Add TVM to Python path.
export PYTHONPATH="$TVM_HOME/python:$TVM_HOME/topi/python:$TVM_HOME/nnvm/python:$TVM_HOME/vta/python:${PYTHONPATH}"
# Add Relay AoT compiler to Python path.
export PYTHONPATH="$RELAY_AOT_HOME:${PYTHONPATH}"
